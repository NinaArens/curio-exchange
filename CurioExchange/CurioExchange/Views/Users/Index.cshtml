@model ICollection<CurioExchange.Models.AspNetUserModel>

@{
    ViewBag.Title = "Players";
}

<h2>Players</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstOrDefault().UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.UserName, "Details", new { userId = item.Id })
        </td>
    </tr>
}

</table>
